package com.capgemini.mywebapp.bean;

public class EmployeeBean {
int empid;
String name;
String salary;
int age;
String password;
String esignation;
public int getEmpid() {
	return empid;
}
public void setEmpid(int empid) {
	this.empid = empid;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getSalary() {
	return salary;
}
public void setSalary(String salary) {
	this.salary = salary;
}
public int getAge() {
	return age;
}
public void setAge(int age) {
	this.age = age;
}
public String getPassword() {
	return password;
}
public void setPassword(String password) {
	this.password = password;
}
public String getEsignation() {
	return esignation;
}
public void setEsignation(String esignation) {
	this.esignation = esignation;
}
}
